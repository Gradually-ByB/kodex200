// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Portfolio {
  id             String @id @default("default")
  kodexQuantity  Int    @default(0)
  kodexAvgPrice  Int    @default(0)
  kodexPrincipal Int    @default(0)
  tigerQuantity  Int    @default(0)
  tigerAvgPrice  Int    @default(0)
  tigerPrincipal Int    @default(0)
  updatedAt      DateTime @updatedAt
}
